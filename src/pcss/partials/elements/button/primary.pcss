.partial[data-partial="elements/button/primary"] {
  border: solid $workbenchColor_3 2px;
  color: $workbenchColor_3;
  background: $workbenchColor_1;
  outline: solid $workbenchColor_1;
  outline-width: 2px;
  &:active {
    color: $workbenchColor_1;
    background: $workbenchColor_2;
    outline: solid $workbenchColor_2;
    border-color: $workbenchColor_1;
  }
}
