@import "../button.pcss";
.partial[data-partial="elements/button/primary"] {
  color: $workbenchColor_3;
  background: $workbenchColor_1;
  outline: solid $workbenchColor_3 2px;
  outline-offset: -4px;
  border: solid $workbenchColor_1 2px;
  outline-width: 2px;
  display: block;
  &:active {
    filter: invert();
  }
}
